{
    "name": "sf-resourcer",
    "displayName": "sf-resourcer",
    "description": "VS Code extension for working with Salesforce Static Resources.",
    "version": "0.0.1",
    "publisher": "JamieSmith",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.staticResrc"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.staticResrc",
                "title": "Work on a Static Resource"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "adm-zip": "^0.4.11",
        "jsforce": "^1.9.1"
    }
}
